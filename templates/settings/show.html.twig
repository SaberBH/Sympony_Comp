{% extends 'base.html.twig' %}

{% block title %}Settings{% endblock %}

{% block body %}
    <h1>Settings</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ setting.id }}</td>
            </tr>
            <tr>
                <th>Aboutus</th>
                <td>{{ setting.aboutus }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ setting.adresse }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ setting.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ setting.telephone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_settings_index') }}">back to list</a>

    <a href="{{ path('app_settings_edit', {'id': setting.id}) }}">edit</a>

    {{ include('settings/_delete_form.html.twig') }}
{% endblock %}
